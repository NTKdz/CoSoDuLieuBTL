<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="profile">
   <children>
      <Label fx:id="name" layoutX="300.0" layoutY="118.0" text="Label" />
      <Label fx:id="email" layoutX="300.0" layoutY="192.0" text="Label" />
      <Label fx:id="password" layoutX="300.0" layoutY="262.0" text="Label" />
      <TextField layoutX="110.0" layoutY="123.0" promptText="change name" />
      <TextField layoutX="110.0" layoutY="188.0" promptText="change email" />
      <TextField layoutX="110.0" layoutY="258.0" promptText="change password" />
      <Button fx:id="confirm" layoutX="110.0" layoutY="333.0" mnemonicParsing="false" onAction="#click" text="confirm" />
      <Label layoutX="110.0" layoutY="14.0" prefHeight="45.0" prefWidth="216.0" text="change your profile">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="back" layoutX="501.0" layoutY="24.0" mnemonicParsing="false" onAction="#back" text="back" />
   </children>
</AnchorPane>
